    <link rel="stylesheet" type="text/css" href="~/AssetsAu/LoginAssets/fonts/font-awesome-4.7.0/css/font-awesome.min.css">

    <link rel="stylesheet" type="text/css" href="~/AssetsAu/LoginAssets/fonts/Linearicons-Free-v1.0.0/icon-font.min.css">

    <link rel="stylesheet" type="text/css" href="~/AssetsAu/LoginAssets/vendor/animate/animate.css">

    <link rel="stylesheet" type="text/css" href="~/AssetsAu/LoginAssets/vendor/css-hamburgers/hamburgers.min.css">

