    <link rel="stylesheet" type="text/css" href="~/AssetsAu/LoginAssets/vendor/select2/select2.min.css">

    <link rel="stylesheet" type="text/css" href="~/AssetsAu/LoginAssets/vendor/daterangepicker/daterangepicker.css">

    <link rel="stylesheet" type="text/css" href="~/AssetsAu/LoginAssets/css/util.css">
    <link rel="stylesheet" type="text/css" href="~/AssetsAu/LoginAssets/css/main.css">
